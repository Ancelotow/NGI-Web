{% extends'base.html.twig' %}

{% block title %}{{ copro.code }}{% endblock %}

{% block body %}
    <div class="container">
        <center>

            <div id="map"></div>
            <script async defer
                    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCtj9bdFee1h3-H2oF_TO5Mvj3PvRFCsCE&callback=init">
            </script>
            <script>

                function init() {
                    var pos = {lat: -20.123, lng: 45.245};
                    var map = new google.maps.Map(document.getElementById('map'), {
                        zoom: 7,
                        center: pos
                    });
                }

            </script>

        </center>
    </div>
{% endblock %}